__lua__
function _init()
 x = 1
 delay = 5
 cls()
end

function _update()
 if delay <= 0 then
 	x += 1
 	if x > 6 then x = 1 end
 	cls()
 	spr(x,0,0)
 	delay = 5
 else
 	delay -= 1
 end
end
